(#! /bin/bash) '
(# PDFcat 0.01 \(c\) 2000, 2001 by Helmar Wodtke) '
(BASENAME="`basename $0`") '
(TMPFILE="$BASENAME.T$$T") '
(VARFILE="$BASENAME.V$$V") '
() '
(AWK="awk") '
() '
($AWK 'BEGIN {) '
(        on=2) '
(        varfile="'"$VARFILE"'") '
(        print "%PDF-1.0") '
(        print "%\\344\\366\\374\\304\\326\\334\\337") '
(        tstream\(\)) '
(}) '
(function tabs\(s\) {) '
(        tn = split\(s, ta, /\\t/\); tr = "") '
(        for \(i=1; i<tn; i++\) {) '
(                tr = tr""ta[i]) '
(                tr = tr""substr\("        ", 1, 8-length\(tr\)%8\)) '
(        }) '
(        return tr""ta[i]) '
(}) '
(function outs\(s\) {) '
(        text[++lp] = s"\\n";) '
(        size += length\(text[lp]\)) '
(}) '
(function out\(s\) {) '
(        gsub\(/[\(\)\\\\]/, "\\\\\\\\&", s\)) '
(        outs\("\("s"\) '\\''"\);) '
(}) '
(function tstream\(\) {) '
(        line=size=lp=0;) '
(        outs\("BT"\)) '
(        outs\("/F1 10 Tf 12 TL"\)) '
(        outs\("82 817 Td"\)) '
(}) '
(/\\14/ {page\(\); next}) '
({gsub\(".\\b",""\); out\(tabs\($0\)\); if \(++line==66\) page\(\)}) '
(function page\(\) {) '
(        outs\("ET"\)) '
(        print on++" 0 obj") '
(        print "<<") '
(        print "/Type /Page") '
(        print "/Parent 1 0 R") '
(        print "/Contents "on" 0 R") '
(        print ">>") '
(        print "endobj") '
(        print on" 0 obj") '
(        print "<< /Length "size" >>") '
(        print "stream") '
(        for \(i=1; i<=lp; i++\) printf\("%s", text[i]\);) '
(        print "endstream") '
(        print "endobj") '
(        on++;) '
(        tstream\(\)) '
(}) '
(END {) '
(        if \(line\) page\(\)) '
(        pages=on/2-1) '
(        print \(font=on++\)" 0 obj") '
(        print "<<") '
(        print "/Type /Font") '
(        print "/Subtype /Type1") '
(        print "/Name /F1") '
(        print "/BaseFont /Courier") '
ET
endstream
endobj
